<?xml version="1.0" encoding="UTF-8"?>
<ns2:ECSpec xmlns:ns2="urn:epcglobal:ale:xsd:1">
    <logicalReaders>
        <logicalReader>warehouse_shelve1</logicalReader>
    </logicalReaders>
    <boundarySpec>
        <repeatPeriod unit="MS">20000</repeatPeriod>
        <duration unit="MS">3000</duration>
        <stableSetInterval unit="MS">0</stableSetInterval>
    </boundarySpec>
    <reportSpecs>
        <reportSpec>
            <reportSet set="CURRENT" />
            <output includeRawHex="true" includeRawDecimal="true" includeEPC="true" />
        </reportSpec>
        <reportSpec reportName="additions">
            <reportSet set="ADDITIONS" />
            <output includeCount="true" includeTag="true" />
        </reportSpec>
        <reportSpec reportName="deletions">
            <reportSet set="DELETIONS" />
            <output includeCount="true" includeTag="true" />
        </reportSpec>
    </reportSpecs>
</ns2:ECSpec>